<Page x:Class="SBL.searchPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SBL"
      mc:Ignorable="d" 
      d:DesignHeight="750" d:DesignWidth="1250" ShowsNavigationUI="False" 
      Title="searchPage">


    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Images/searchPage.png"/>
        </Grid.Background>
        <TextBox Name ="Search_Box" HorizontalAlignment="Left" Height="70" Margin="350,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Center"  Width="484" FontSize="40"/>
        <Button   HorizontalAlignment="Left" Height="73" Margin="814,30,0,0" VerticalAlignment="Center" Width="74" Click="SearchButton_Click">
            <Button.Background>
                <ImageBrush ImageSource="Images/Search_Button.jpg"/>
            </Button.Background>
        </Button>
        <Button x:Name="btnBack" Margin="1026,36,34,634" Click="btnBack_Click" RenderTransformOrigin="-0.196,0.651" >
            <Button.Background>
                <ImageBrush ImageSource="Images/btnBack.png"/>
            </Button.Background>
        </Button>


    </Grid>
</Page>
